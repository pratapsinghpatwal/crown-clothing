{
  "name": "crown-clothing-server",
  "version": "1.0.0",
  "engines": {
    "node": "v16.14.0",
    "npm": "8.5.3"
  },
  "scripts": {
    "client": "cd client && npm start",
    "server": "nodemon server.js",
    "build": "cd client && npm run build",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "start": "node server.js",
    "heroku-postbuild": "cd client && npm install --force && npm install --only=dev --no-shrinkwrap --force && npm run build"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "dotenv": "7.0.0",
    "express": "^4.16.4",
    "stripe": "6.28.0"
  },
  "devDependencies": {
    "concurrently": "^7.0.0",
    "nodemon": "^2.0.15"
  },
  "description": "To view a demo , **[click here](https://crown-live-ztm.herokuapp.com)**",
  "main": "server.js",
  "author": "",
  "license": "ISC"
}
